                                <attributedString key="attributedText">
                                    <fragment content="ACS311: PRINCIPLES OF PROGRAMMING">
                                        <attributes>
                                            <font key="NSFont" size="12" name="TimesNewRomanPS-BoldMT"/>
                                            <font key="NSOriginalFont" size="12" name="TimesNewRomanPS-BoldMT"/>
